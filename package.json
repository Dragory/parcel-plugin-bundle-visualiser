{
  "name": "parcel-plugin-bundle-visualiser",
  "version": "0.1.0",
  "description": "A plugin for the parcel bundler to visualise bundle contents",
  "author": "Greg Tillbrook",
  "license": "MIT",
  "repository": "gregtillbrook/parcel-plugin-bundle-visualiser",
  "engines": {
    "node": ">=6"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run clean && npm run lint && npm run build",
    "clean": "rimraf dist",
    "lint": "node node_modules/eslint/bin/eslint.js src/index.js",
    "build": "cd src && node ../node_modules/babel-cli/bin/babel './*.js'  --out-dir '../dist'",
    "test": "echo todo"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "parcel",
    "plugin",
    "bundler",
    "report",
    "visualise",
    "visualize"
  ]  
}
